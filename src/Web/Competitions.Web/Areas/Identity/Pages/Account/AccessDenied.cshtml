@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Достъп отказан";
    ViewData["Keywords"] = "Достъп, Неоторизиран";
    ViewData["Description"] = "Достъп отказан, потребителят не е оторизиран да получи дадения ресурс.";
}
<div class="center-custom">
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <img src="~/images/errors/401.jpg" class="error-code-image" alt="Неоторизиран опит за достъп до ресурс">
    <br>
    <a class="btn btn-primary" asp-area="" asp-controller="Home" asp-action="Index" role="button">Обратно към начална страница</a>
</div>
